{
  "info": {
    "name": "AI Image Editor API",
    "description": "FastAPI backend for AI-powered image editing using FAL AI",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/x-www-form-urlencoded"
              }
            ],
            "body": {
              "mode": "urlencoded",
              "urlencoded": [
                {
                  "key": "email",
                  "value": "user@example.com",
                  "type": "text"
                },
                {
                  "key": "password",
                  "value": "yourpassword123",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/auth/register",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["auth", "register"]
            },
            "description": "Register a new user account"
          }
        },
        {
          "name": "Login User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/x-www-form-urlencoded"
              }
            ],
            "body": {
              "mode": "urlencoded",
              "urlencoded": [
                {
                  "key": "username",
                  "value": "user@example.com",
                  "type": "text"
                },
                {
                  "key": "password",
                  "value": "yourpassword123",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/auth/login",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["auth", "login"]
            },
            "description": "Login and get access token"
          }
        }
      ]
    },
    {
      "name": "Jobs",
      "item": [
        {
          "name": "Create Job - Text to Image",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "prompt",
                  "value": "a beautiful sunset over mountains with dramatic clouds",
                  "type": "text",
                  "description": "Prompt for image generation"
                },
                {
                  "key": "strength",
                  "value": "0.7",
                  "type": "text",
                  "description": "Ignored for text-to-image"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/api/jobs",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["api", "jobs"]
            },
            "description": "Create a text-to-image job (no image upload)"
          }
        },
        {
          "name": "Create Job - Image to Image",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "prompt",
                  "value": "make it look like a cartoon",
                  "type": "text",
                  "description": "Editing instructions for the image"
                },
                {
                  "key": "image",
                  "type": "file",
                  "src": "/path/to/your/image.jpg",
                  "description": "Image file to edit"
                },
                {
                  "key": "strength",
                  "value": "0.8",
                  "type": "text",
                  "description": "How much to change the image (0-1)"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/api/jobs",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["api", "jobs"]
            },
            "description": "Edit an existing image with AI using a prompt"
          }
        },
        {
          "name": "List Jobs - Paginated",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/jobs?page=1&limit=10",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["api", "jobs"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            },
            "description": "Get paginated list of jobs for current user"
          }
        },
        {
          "name": "List Jobs - Second Page",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/jobs?page=2&limit=10",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["api", "jobs"],
              "query": [
                {
                  "key": "page",
                  "value": "2"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            },
            "description": "Get second page of jobs (items 11-20)"
          }
        },
        {
          "name": "Get Specific Job",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/jobs/1",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["api", "jobs", "1"]
            },
            "description": "Get details of a specific job by ID"
          }
        }
      ]
    },
    {
      "name": "Health & Info",
      "item": [
        {
          "name": "Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/health",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["health"]
            },
            "description": "Check if API is running"
          }
        },
        {
          "name": "Root Endpoint",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": [""]
            },
            "description": "Root endpoint"
          }
        }
      ]
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8000",
      "type": "string"
    },
    {
      "key": "access_token",
      "value": "YOUR_ACCESS_TOKEN_HERE",
      "type": "string"
    }
  ],
  "event": [
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Set access token from login response",
          "if (pm.response.code === 200) {",
          "    var jsonData = pm.response.json();",
          "    if (jsonData.access_token) {",
          "        pm.collectionVariables.set(\"access_token\", jsonData.access_token);",
          "        console.log(\"Access token saved: \" + jsonData.access_token);",
          "    }",
          "}",
          "",
          "// Basic tests",
          "pm.test(\"Status code is successful\", function () {",
          "    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
          "});",
          "",
          "pm.test(\"Response time is acceptable\", function () {",
          "    pm.expect(pm.response.responseTime).to.be.below(30000);",
          "});"
        ]
      }
    }
  ]
}